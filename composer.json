{
  "name": "cafesource/helper",
  "description": "The cafesource helper",
  "keywords": [
    "cafesource",
    "Helper"
  ],
  "license": "MIT",
  "homepage": "https://cafesource.net",
  "authors": [
    {
      "name": "Mahdi akbari",
      "email": "mahdi75akbari@gmail.com"
    }
  ],
  "require": {
    "php": "^7.4|8.0",
    "ext-json": "*",
    "ext-curl": "*",
    "ext-mbstring": "*",
    "ext-openssl": "*"
  },
  "provide": {
    "psr/container-implementation": "1.0"
  },
  "autoload": {
    "psr-4": {
      "Cafesource\\Helper\\": "src/"
    },
    "files": [
      "helpers/helpers.php"
    ]
  },
  "autoload-dev": {
    "files": []
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.x-dev"
    }
  },
  "config": {
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
